#include <stdio.h>

int main() {
    char name[100];
    int arrears;
    int cgpa;

    // Reading input
    scanf("%s", name);
    scanf("%d", &arrears);
    scanf("%d", &cgpa);

    // Printing the student's name
    printf("Name of the Student:%s\n", name);

    // Eligibility conditions
    if ((arrears == 1 && cgpa > 70) || (arrears <= 2 && cgpa > 75)) {
        printf("%s is Eligible for Placement\n", name);
    } else {
        printf("%s is Not Eligible for Placement\n", name);
    }

    return 0;
}
